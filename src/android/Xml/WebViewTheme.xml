<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="WebViewTheme" parent="@style/Theme.AppCompat.NoActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/WebViewAnimation</item>
    </style>
    <style name="WebViewAnimation">
        <item name="android:windowEnterAnimation">@animator/slide_left</item>
        <item name="android:windowExitAnimation">@animator/fade_out</item>
    </style>
</resources>